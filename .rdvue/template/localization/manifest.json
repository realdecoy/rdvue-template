{
  "version": 2,
  "name": "localization",
  "group": false,
  "description": "Generate files needed for basic localization.",
  "sourceDirectory": "./",
  "installDirectory": "./",
  "files": [
    {
      "source": "config/i18n.ts",
      "target": "config/i18n.ts"
    },
    {
      "source": "views/hello-i18n/hello-i18n.vue",
      "target": "views/hello-i18n/hello-i18n.vue"
    },
    {
      "source": "locales/en.json",
      "target": "locales/en.json"
    },
    {
      "source": "locales/es.json",
      "target": "locales/es.json"
    }
  ],
  "packages": {
    "dependencies": [
      "vue-i18n@9.2.2"
    ],
    "devDependencies": [
      "vue-cli-plugin-i18n@1.0.1"
    ]
  },
  "routes": [
    {
      "path": "'/hello-i18n'",
      "name": "'i18nTest'",
      "component": {
        "import": "() => import('@/views/hello-i18n/hello-i18n.vue')",
        "webpackChunkName": "'i18nTest'"
      }
    }
  ],
  "vueOptions": {
    "i18n": {
      "locale": "'en'",
      "fallbackLocale": "'es'",
      "localeDir": "'locales'",
      "enableInSFC": "false"
    }
  },
  "modules": {
    "i18n": "`require('../src/config/i18n').default`"
  },
  "main": {
    "modules": [
      "i18n"
    ],
    "imports": [
      "import i18n from '@/config/i18n';"
    ]
  }
}