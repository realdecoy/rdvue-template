{
  "name": "__PROJECT_NAME__",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "audit": "npm audit --prod",
    "clear-library": "rimraf .library",
    "clone-library": "git clone https://github.com/realdecoy/design-system-components.git .library",
    "clean-library": "cd .library && rimraf README.md && rimraf .git",
    "upgrade-rdvue": "npx rdvue@latest upgrade",
    "setup-ds-webpack": "cp .rdvue/template/design-system/webpack.config.ts node_modules/design-system/webpack.config.ts",
    "setup-ds-library": "run-s clear-library clone-library clean-library",
    "launch-ds": "cd node_modules/design-system && npm install && npm run serve:rdvue -- --port 9000",
    "serve": "cross-env NODE_ENV=development webpack serve",
    "build": "cross-env NODE_ENV=production webpack build",
    "serve:build": "cd dist && ws --https --spa index.html -z -p 9000",
    "serve:docker": "docker run -it -p 8080:8080 -v $(pwd):/app -w /app --rm node bash -c \"npm run serve\"",
    "serve:ds": "run-s setup-ds-webpack launch-design-system",
    "lint": "tslint -p .",
    "lint:fix": "tslint --fix -p .",
    "test:e2e": "echo \"Error: no e2e test specified\" && exit 1",
    "test:unit": "echo \"Error: no unit test specified\" && exit 1"
  },
