{
  "name": "__PROJECT_NAME__",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clear-library": "rimraf .library",
    "clone-library": "git clone https://github.com/realdecoy/design-system-components.git .library",
    "clean-library": "cd .library && rimraf README.md && rimraf .git",
    "upgrade-rdvue": "npx rdvue@latest upgrade",
    "setup-ds-webpack": "cp .rdvue/template/design-system/webpack.config.ts node_modules/design-system/webpack.config.ts",
    "setup-ds-library": "run-s clear-library clone-library clean-library",
    "postinstall": "run-s upgrade-rdvue",
    "serve": "vue-cli-service serve",
    "serve-static": "cd dist && ws --spa index.html -z",
    "build": "vue-cli-service build",
    "lint": "npx tslint -p .",
    "lint:fix": "npx tslint --fix -p .",
    "test:e2e": "echo \"Error: no e2e test specified\" && exit 1",
    "test:unit": "echo \"Error: no unit test specified\" && exit 1",
    "launch-design-system": "cd node_modules/design-system && npm install && npm run serve:rdvue -- --port 9000",
    "serve:design-system": "run-s setup-ds-webpack launch-design-system"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "axios": "^0.21.1",
    "core-js": "^2.6.11",
    "deepmerge": "^4.2.2",
    "favicons": "^6.2.0",
    "js-beautify": "^1.13.0",
    "log-symbols": "^4.0.0",
    "register-service-worker": "^1.5.2",
    "tslib": "^2.2.0",
    "vue": "^2.5.22",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-module-decorators": "^0.10.1",
    "ws": "^7.4.5"
  },
  "devDependencies": {
    "@types/node": "^10.14.8",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-typescript": "^3.4.0",
    "@vue/cli-service": "^4.3.1",
    "autoprefixer": "^9.7.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-preset-vue": "^2.0.2",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^7.1.0",
    "tailwindcss": "^1.4.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-immutable": "^6.0.1",
    "typescript": "^3.0.0",
    "vue-template-compiler": "^2.5.21",
    "webpack-bundle-analyzer": "^3.7.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}